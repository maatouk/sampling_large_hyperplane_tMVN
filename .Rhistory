# ------------------------------------------------------------
# Sampler configuration
# ------------------------------------------------------------
Tbin <- 5000  # Burn-in samples
Te   <- 5000  # Desired samples
# ------------------------------------------------------------
# Restricted domain: unit l1-ball
# ------------------------------------------------------------
N <- 2  # Dimension of the target distribution
# Linear inequality matrix and bounds: c <= R*x <= d
R <- matrix(c(1, 1,
1, -1), nrow = 2, byrow = TRUE)
c <- matrix(-1, nrow = N, ncol = 1)
d <- matrix( 1, nrow = N, ncol = 1)
# ------------------------------------------------------------
# Precision matrix and mean vector
# ------------------------------------------------------------
Q  <- diag(N)
mu <- matrix(1, nrow = N, ncol = 1)
# ------------------------------------------------------------
# Run sampler
# ------------------------------------------------------------
X <- CMG_TMG_BTC(Q, mu, R, c, d, Tc = Te, Tbin = Tbin)
